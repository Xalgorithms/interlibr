{
  "id": "/respond",
  "cmd": "npm run start-production",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "xalgorithms/xadf-respond-service:latest-development",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 4200,
          "hostPort": 5000
        }
      ]
    }
  },
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "env": {
    "MARATHON_TASK_LAUNCH_TIMEOUT": "900000"
  },
  "acceptedResourceRoles": ["slave_public"]
}