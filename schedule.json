{
  "id": "/schedule",
  "cmd": "bundle exec puma",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "xalgorithms/xadf-schedule-service:latest-development",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 9292,
          "hostPort": 4000
        }
      ]
    }
  },
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "env": {
    "MARATHON_TASK_LAUNCH_TIMEOUT": "900000"
  },
  "acceptedResourceRoles": ["slave_public"]
}